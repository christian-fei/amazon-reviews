    "init": "mkdir -p html ; mkdir -p json; mkdir -p screenshots",
    "start": "./bin.js",
    "server": "node server.js",
    "test": "rm -rf html/amazon/B07PHPXHQS* ; rm -rf json/amazon/B07PHPXHQS* ; DEBUG='mega-scraper:*' ava --fail-fast -c 1 -v",
    "migrate": "mkdir -p migrations; DEBUG='mega-scraper:*' node migrate.js",
    "bin": "DEBUG='mega-scraper:*' node --max-old-space-size=8192 bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christian-fei/mega-scraper.git"
